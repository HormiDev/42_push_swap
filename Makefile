test1:
	@echo "test 1"
	@echo parámetros no numéricos
	@if [ "$$(./$(NAME) $(ARGS1) 2>&1)" = "Error" ]; then \
		echo "Error $(VERDE)[OK]$(NC)\n"; \
	else \
		echo "Error $(ROJO)[KO]$(NC)\n"; \
	fi

test2:
	@echo "test 2"
	@echo "numérico duplicado"
	@if [ "$$(./$(NAME) $(ARGS2) 2>&1)" = "Error" ]; then \
		echo "Error $(VERDE)[OK]$(NC)\n"; \
	else \
		echo "Error $(ROJO)[KO]$(NC)\n"; \
	fi

